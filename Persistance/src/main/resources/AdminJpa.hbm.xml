<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
    <class name="AdminJpa"
           table="Admins">
        <id name="id" column="ID">
            <generator class="identity"/>
        </id>
        <property
                name="username"
                column="username"/>
        <property
                name="passwordHash"
                column="passwordHash"/>

    </class>
</hibernate-mapping>
